{
    "minimum-stability": "dev",
    "prefer-stable":     true,
    "name":              "mpalourdio/mpa-zend-skeleton-heavy",
    "description":       "Customized Skeleton Application for ZF2",
    "license":           "MIT",
    "keywords":          [
        "framework",
        "zf2",
        "skeleton",
        "heavy"
    ],
    "homepage":          "http://framework.zend.com/",
    "require":           {
        "php":                                     ">=5.5",
        "zendframework/zendframework":             "2.3.*",
        "doctrine/doctrine-orm-module":            "dev-master",
        "zendframework/zend-developer-tools":      "dev-master",
        "mpalourdio/mpa-custom-doctrine-hydrator": "0.*",
        "mpalourdio/mpa-firephp-wrapper":          "0.*",
        "ocramius/ocra-cached-view-resolver":      "3.0.*"
    },
    "scripts": {
        "post-install-cmd": [
            "bower install"
        ],
        "post-update-cmd": [
            "bower update"
        ]
    },
    "autoload":          {
        "psr-4": {
            "Application\\":     "module/Application/src",
            "ApplicationTest\\": "module/Application/tests"
        }
    }
}
